fn main() {
    let x = 1;
    x++;
    assert(x == 2);
    assert(++x == 3);
    x--;
    assert(x == 2);
    assert(--x == 1);

    let a = [0];
    a[0]++;
    assert(a[0] == 1);
    assert(++a[0] == 2);

    let m = {"k": 1};
    m.k++;
    assert(m.k == 2);
    assert(++m.k == 3);

    print("ok\n");
}